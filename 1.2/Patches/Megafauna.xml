<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/ThingDef[defName="Enhydriodon"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationConditional">
      <xpath>/Defs/ThingDef[defName="Enhydriodon"]/comps</xpath>
      <match Class="PatchOperationAdd">
        <xpath>/Defs/ThingDef[defName="Enhydriodon"]/comps</xpath>
        <value>
          <li Class="AnimalBehaviours.CompProperties_DigWhenHungry">
            <customThingToDig>AEXP_RawFish</customThingToDig>
            <customAmountToDig>3</customAmountToDig>
          </li>
        </value>
      </match>
      <nomatch Class="PatchOperationAdd">
        <xpath>/Defs/ThingDef[defName="Enhydriodon"]</xpath>
        <value>
          <comps>
            <li Class="AnimalBehaviours.CompProperties_DigWhenHungry">
              <customThingToDig>AEXP_RawFish</customThingToDig>
              <customAmountToDig>3</customAmountToDig>
            </li>
          </comps>
        </value>
      </nomatch>
    </match>
  </Operation>
